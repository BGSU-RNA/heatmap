<div class='panel panel-default'>
  <div class='panel-heading'>
    <strong>Base Combinations</strong>
  </div>

  <table class='table table-bordered table-hover table-condensed'>
    <thead>
      <tr>
        <th>Sequence</th>
        <th>IDI Group</th>
        <th>Count</th>
        <th>PDB</th>
        <th>Resolution</th>
        <th>Nucleotides</th>
      </tr>
    </thead>

    <tbody>
      {{#each items}}
      <tr id='{{id}}' class='jmol-toggle {{class}}' data-sequence='{{sequence}}'>
        <td><i class="fa {{icon}}"></i>&nbsp;{{sequence}}</td>
        <td>{{group}}</td>
        <td>{{count}}</td>
        <td>{{pdb}}</td>
        <td>{{resolution}}</td>
        <td>{{units.[0]}}, {{units.[1]}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
